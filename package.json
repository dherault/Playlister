{
  "name": "playlister",
  "version": "0.0.0",
  "description": "Combine anything to create a cool playlist",
  "main": "src/server/static_server.js",
  "repository": "https://github.com/dherault/Playlister.git",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "print_node_info": "node ./src/server/utils/printNodeInfo.js",
    "start": "clear && npm run print_node_info && npm run all",
    "all": "npm run webpack_server & npm run asset_server & npm run rendering_server & npm run api_server & npm run websocket_server & npm run kvs_server & npm run image_server",
    "main": "npm run webpack_server & npm run rendering_server",
    "api": "npm run api_server",
    "tierce": "npm run asset_server & npm run kvs_server & npm run websocket_server & npm run image_server",
    "webpack_server": "nodemon -q -x \"babel-node\" -w ./src/server/utils -w ./src/server/webpack_server -e js ./src/server/webpack_server/webpack_server.js",
    "rendering_server": "nodemon -q -x \"babel-node\" -w ./src/server/utils -w ./src/server/rendering_server -w ./src/shared -e js ./src/server/rendering_server/rendering_server.js",
    "asset_server": "nodemon -q -x \"babel-node\" -w ./src/server/utils -w ./src/server/asset_server -e js ./src/server/asset_server/asset_server.js",
    "api_server": "nodemon -q -x \"babel-node\" -w ./src/server/utils -w ./src/server/api_server -w ./src/server/database -w ./src/shared -i ./src/shared/components -e js ./src/server/api_server/api_server.js",
    "websocket_server": "nodemon -q -x \"babel-node\" -w ./src/server/utils -w ./src/server/websocket_server -e js ./src/server/websocket_server/websocket_server.js",
    "kvs_server": "nodemon -q -x \"babel-node\" -w ./src/server/utils -w ./src/server/kvs_server -e js ./src/server/kvs_server/kvs_server.js",
    "image_server": "nodemon -q -x \"babel-node\" -w ./src/server/utils -w ./src/server/image_server -e js ./src/server/image_server/image_server.js",
    "clean_generated_images": "rm src/server/public/images/generated/*",
    "cgi": "npm run clean_generated_images",
    "pi": "sudo npm i -g nodemon babel-cli"
  },
  "author": "David HÃ©rault",
  "license": "GPL-3.0",
  "devDependencies": {
    "babel-loader": "^6.2.1",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-2": "^6.3.13",
    "css-loader": "^0.23.1",
    "exports-loader": "^0.6.2",
    "imports-loader": "^0.6.5",
    "postcss-loader": "^0.8.0",
    "react-hot-loader": "^1.3.0",
    "style-loader": "^0.13.0",
    "webpack": "^1.12.12",
    "webpack-dev-server": "^1.14.1"
  },
  "dependencies": {
    "bcrypt": "^0.8.5",
    "catbox": "^7.1.0",
    "catbox-redis": "^1.0.7",
    "chalk": "^1.1.1",
    "gm": "^1.21.1",
    "hapi": "^13.0.0",
    "hapi-auth-jwt2": "^5.3.2",
    "inert": "^3.2.0",
    "lodash": "^4.2.1",
    "node-fetch": "^1.3.3",
    "react": "^0.14.7",
    "react-dom": "^0.14.7",
    "react-redux": "^4.1.2",
    "react-router": "2.0.0-rc5",
    "react-router-redux": "^3.0.0",
    "redux": "^3.1.6",
    "rethinkdb": "^2.2.1",
    "socket.io": "^1.4.5",
    "socket.io-client": "^1.4.5",
    "uuid": "^2.0.1",
    "whatwg-fetch": "^0.11.0"
  }
}
